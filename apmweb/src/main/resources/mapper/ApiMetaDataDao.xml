<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="apm.web.dao.apmmysql.ApiMetaDataDao">
    <select id="getApiIdByApiInFo" parameterType="java.lang.String" resultMap="ApiMethodMap">
        SELECT * from api_meta_data amd1 WHERE  amd1.apiInfo = #{apiInfo};
    </select>

    <resultMap id="ApiMethodMap" type="apm.web.beans.module.ApiMethod">
        <result property="agentId" column="agentId"/>
        <result property="apiMetaId" column="apiMetaId"/>
        <result property="agentStartTime" column="agentStartTime"/>
        <result property="apiId" column="apiId"/>
        <result property="apiInfo" column="apiInfo"/>
        <result property="line" column="line"/>
        <result property="type" column="type"/>

    </resultMap>

</mapper>
